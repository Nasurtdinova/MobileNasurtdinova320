<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TestNasurtdinova320.Contacts"
             Title="Контакты">
    <ContentPage.Content>
        <StackLayout Padding="50" Spacing="30">
            <StackLayout>
                <Label Text="Телефон" TextColor="black"></Label>
                <Frame BackgroundColor="#DBDBDB">
                    <Entry x:Name="phoneEntry" />
                </Frame>
            </StackLayout>
           
            <StackLayout>
                <Label Text="Телефон" TextColor="black"></Label>
                <Frame BackgroundColor="#DBDBDB">
                    <Entry x:Name="phone2Entry" />
                </Frame>
            </StackLayout>
               
            <StackLayout>
                <Label Text="Email" TextColor="black"></Label>
                <Frame BackgroundColor="#DBDBDB" >
                    <Entry x:Name="emailEntry" />
                </Frame>
            </StackLayout>

            <StackLayout>
                <Label Text="Адрес" TextColor="black"></Label>
                <Frame BackgroundColor="#DBDBDB">
                    <Entry x:Name="addressEntry" />
                </Frame>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>